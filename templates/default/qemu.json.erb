{
  <%- @mappings.each do |name, data| -%>
    "<%= name %>": {
      "interface": "<%= data["interface"] %>",
      "private_ip": "<%= data["ip"] %>",
      "port_map": {
        "tcp": <%= (data["tcp"] || []).inspect %>,
        "udp": <%= (data["udp"] || []).inspect %>
      }
    }<%- unless @mappings.keys.last == name -%>,<%- end -%>
  <%- end -%>
}
